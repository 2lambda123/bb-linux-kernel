From 7e523263915e08c138392c7cada4c704d643bec3 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Mon, 8 Jan 2018 11:54:08 -0600
Subject: [PATCH] dtc: disable TI overlays

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 arch/arm/Makefile          | 15 ++++++++-------
 arch/arm/boot/dts/Makefile |  2 +-
 2 files changed, 9 insertions(+), 8 deletions(-)

diff --git a/arch/arm/Makefile b/arch/arm/Makefile
index 0e7d939528b7..d4db7dc26412 100644
--- a/arch/arm/Makefile
+++ b/arch/arm/Makefile
@@ -347,19 +347,20 @@ $(INSTALL_TARGETS):
 %.dtb: | scripts
 	$(Q)$(MAKE) $(build)=$(boot)/dts MACHINE=$(MACHINE) $(boot)/dts/$@
 
-%.dtbo: dtspath = $(dir $@)
+#%.dtbo: dtspath = $(dir $@)
 
-%.dtbo: | scripts
-	$(Q)$(MAKE) $(build)=$(boot)/dts/$(dtspath) MACHINE=$(MACHINE) $(boot)/dts/$(dtspath)$(notdir $@)
+#%.dtbo: | scripts
+#	$(Q)$(MAKE) $(build)=$(boot)/dts/$(dtspath) MACHINE=$(MACHINE) $(boot)/dts/$(dtspath)$(notdir $@)
 
-%.itb: itspath = $(dir $@)
+#%.itb: itspath = $(dir $@)
 
-%.itb: | scripts
-	$(Q)$(MAKE) $(build)=$(boot)/dts/$(itspath) MACHINE=$(MACHINE) $(boot)/dts/$(itspath)$(notdir $@)
+#%.itb: | scripts
+#	$(Q)$(MAKE) $(build)=$(boot)/dts/$(itspath) MACHINE=$(MACHINE) $(boot)/dts/$(itspath)$(notdir $@)
 
 PHONY += dtbs dtbs_install
 
-dtbs: prepare scripts zImage
+#dtbs: prepare scripts zImage
+dtbs: prepare scripts
 	$(Q)$(MAKE) $(build)=$(boot)/dts
 
 dtbs_install:
diff --git a/arch/arm/boot/dts/Makefile b/arch/arm/boot/dts/Makefile
index 3c5fcab544f4..a21b3aa3e952 100644
--- a/arch/arm/boot/dts/Makefile
+++ b/arch/arm/boot/dts/Makefile
@@ -1096,4 +1096,4 @@ dtb-$(CONFIG_OF_ALL_DTBS) := $(patsubst $(dtstree)/%.dts,%.dtb, $(wildcard $(dts
 always		:= $(dtb-y)
 clean-files	:= *.dtb
 
-subdir-y	+= $(dts-dirs-y)
+#subdir-y	+= $(dts-dirs-y)
