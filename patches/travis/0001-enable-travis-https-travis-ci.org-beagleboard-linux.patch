From e33c5631b82f3ea851f60364e3d1b1b8b8d5b223 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 14 Apr 2016 13:59:08 -0500
Subject: [PATCH] enable: travis: https://travis-ci.org/beagleboard/linux

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 .travis.yml | 14 ++++++++++++++
 1 file changed, 14 insertions(+)
 create mode 100644 .travis.yml

diff --git a/.travis.yml b/.travis.yml
new file mode 100644
index 0000000..6a982da
--- /dev/null
+++ b/.travis.yml
@@ -0,0 +1,14 @@
+dist: trusty
+language: c
+compiler: gcc
+cache: ccache
+env:
+  - ARCH=arm CROSS_COMPILE=arm-linux-gnueabihf- LOCALVERSION=-git$TRAVIS_COMMIT
+addons:
+  apt:
+    packages:
+    - gcc-arm-linux-gnueabihf
+    - lzop
+script:
+  - make bb.org_defconfig
+  - make -j4 CROSS_COMPILE="ccache arm-linux-gnueabihf-" zImage modules dtbs
-- 
2.8.0.rc3

