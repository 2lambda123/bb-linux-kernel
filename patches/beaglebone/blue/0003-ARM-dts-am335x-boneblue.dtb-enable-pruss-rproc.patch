From 1a59d6e2ee85fbffd03a13d9b5c3bd50d6c7533c Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Thu, 6 Oct 2016 12:15:45 -0500
Subject: [PATCH 3/3] ARM: dts: am335x-boneblue.dtb: enable pruss/rproc

Signed-off-by: Robert Nelson <robertcnelson@gmail.com>
---
 arch/arm/boot/dts/am335x-boneblue.dts | 14 +-------------
 1 file changed, 1 insertion(+), 13 deletions(-)

diff --git a/arch/arm/boot/dts/am335x-boneblue.dts b/arch/arm/boot/dts/am335x-boneblue.dts
index 66075a9..6152964 100644
--- a/arch/arm/boot/dts/am335x-boneblue.dts
+++ b/arch/arm/boot/dts/am335x-boneblue.dts
@@ -16,25 +16,13 @@
 
 #define BLUE_IO(x, y) AM33XX_IOPAD((x)*4+0x800, (y))
 
-/* pruss: pick one: */
-
 /*
  * /etc/modprobe.d/pruss-blacklist.conf
  *
  * blacklist uio_pruss
  */
 
-/* #include "am33xx-pruss-rproc.dtsi" */
-
-/*
- * /etc/modprobe.d/pruss-blacklist.conf
- *
- * blacklist pruss
- * blacklist pruss_intc
- * blacklist pru-rproc
- */
-
-/* #include "am33xx-pruss-uio.dtsi" */
+#include "am33xx-pruss-rproc.dtsi"
 
 / {
 	model = "TI AM335x BeagleBone Blue";
-- 
